<div class="page-header">

</div>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>
      </th>
    </tr>
  </thead>
  <tbody>
      <% @user.each do |user| %>
        <tr>
          <td class="col-md-3">
            <%= link_to "#{user.full_name}", user_path(user) %>
          </td>
          <td class="col-md-1">
            <%= mail_to user.email %>
            </td>
        </tr>
        <% end %>
    </tbody>
  </table>
