<div class= "container">
  <div class= "page-header">
  </div>
<h1> List of Patients</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <tr>
  <% @users.each do |user| %>
      <td><%= link_to user.full_name %></td>
      <td><%= mail_to user.email %></td>
      <% end %>
    </tr>
  </tbody>
</table>
</div>
