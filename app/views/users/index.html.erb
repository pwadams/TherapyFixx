<div class= "container">
  <div class= "page-header">
  </div>
<h1> List of Patients</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td><%= link_to user.full_name, user_path(user) %></td>
      <td><%= mail_to user.email %></td>
    </tr>
  </tbody>
  <% end %>
</table>
</div>
